{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "esnext",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "outDir": "./dist",
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./src/front/*"
      ],
      "@/*": [
        "./src/front/*"
      ]
    },
    "types": [
      "@types/node",
      "@nuxt/types",
      "@nuxtjs/vuetify"
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}
